{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/api/index.js"
      }
    ],
    "build": {
      "env": {
        "NODE_ENV": "production"
      },
      "commands": {
        "prebuild": "npm run build"
      }
    }
  }
  