{
    "name": "github-image-bed",
    "version": 2,
    "builds": [
        {
            "src": "/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ],
    "description": "",
    "main": "index.js",
    "scripts": {
        "build-css": "sass --watch src/scss:public/css",
        "copy-html": "cpx \"src/index.html\" public --watch",
        "build": "cpx \"src/index.html\" public & sass src/scss:public/css",
        "start": "node --watch index.js"
    },
    "keywords": [],
    "author": "Kevin Zhong",
    "license": "MIT",
    "dependencies": {
        "axios": "^1.7.3",
        "copyfiles": "^2.4.1",
        "cpx": "^1.5.0",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "multer": "1.4.5-lts.1",
        "sass": "^1.77.8"
    }
}